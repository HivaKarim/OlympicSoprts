                         Event ON Athlete.ID = Event.ID INNER JOIN
                         Gender ON Athlete.Gender_ID = Gender.ID INNER JOIN
                         Host ON City.ID = Host.City_ID INNER JOIN
                         Medal ON Athlete.ID = Medal.ID INNER JOIN
                         Result ON Athlete.ID = Result.Athlete_ID AND Event.ID = Result.Event_ID AND Host.ID = Result.Host_ID AND Medal.ID = Result.Medal_ID INNER JOIN
                         Season ON Athlete.ID = Season.ID INNER JOIN
